@{
    ViewData["Title"] = "About";
}
<h2>@ViewData["Title"]</h2>

<dt>ID Token</dt>
<dd>@ViewData["idToken"]</dd>

<dt>Access Token</dt>
<dd>@ViewData["accessToken"]</dd>

<dl>
    @foreach (var claim in User.Claims)
    {
        <dt>@claim.Type</dt>
        <dd>@claim.Value</dd>
    }
</dl>

<h3>@ViewData["email"]</h3>